{
  "expo": {
    "name": "Lacrosse Faceoff Trainer",
    "slug": "lacrosse-faceoff-trainer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jljackson222.lacrossefaceofftrainer",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "AVAudioSessionCategory": "AVAudioSessionCategoryPlayback",
        "AVAudioSessionCategoryOptions": [
          "AVAudioSessionCategoryOptionMixWithOthers"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jljackson222.lacrossefaceofftrainer",
      "versionCode": 1,
      "permissions": [
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "updates": {
      "url": "https://u.expo.dev/3655c7ed-55eb-4d91-904b-e1cdb3b9ba54"
    },
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "3655c7ed-55eb-4d91-904b-e1cdb3b9ba54"
      }
    },
    "owner": "jljackson222",
    "plugins": [
      "expo-font",
      "expo-audio"
    ]
  }
}
